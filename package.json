{
  "name": "swagger-to-typescript-client",
  "version": "1.0.8",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "UNLICENSED",
  "scripts": {
    "start": "node_modules/.bin/ts-node index.ts",
    "copy-assets": "cp -r template dist/template/",
    "build": "rm -rf dist && tsc -p . --skipLibCheck && yarn copy-assets",
    "prepublishOnly": "yarn build",
    "test": "NODE_ENV=test jest --runInBand --forceExit",
    "lint": "node_modules/.bin/eslint \\\"src/**/*.ts\\\" \\\"./index.ts\\\" \\\"__tests__/**/*.ts\\\""
  },
  "bin": {
    "gen-sdk": "dist/index.js"
  },
  "files": [
    "dist/*",
    "package.json",
    "yarn.lock"
  ],
  "dependencies": {
    "camelcase": "6.2.0",
    "ncp": "^2.0.0",
    "openapi-client-axios-typegen": "3.14.1",
    "openapi-types": "^9.3.0",
    "prompts": "2.4.2",
    "rimraf": "3.0.2",
    "swagger-parser": "10.0.3",
    "ts-node": "10.3.0",
    "typescript": "4.4.4",
    "yargs": "17.2.1"
  },
  "devDependencies": {
    "@types/eslint": "^7.28.1",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/jest": "^27.0.2",
    "@types/ncp": "^2.0.5",
    "@types/node": "^16.10.4",
    "@types/prettier": "^2.4.1",
    "@types/prompts": "^2.0.14",
    "@types/rimraf": "3.0.2",
    "@types/swagger-schema-official": "^2.0.22",
    "@types/yargs": "17.0.3",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.2",
    "jest": "^27.2.5",
    "lint-staged": "^11.2.3",
    "prettier": "^2.4.1",
    "swagger-schema-official": "^2.0.0-bab6bed",
    "ts-jest": "next"
  },
  "lint-staged": {
    "src/**/*.{ts,json}": "prettier --write",
    "__tests__/**/*.{ts}": "prettier --write",
    "./index.ts": "prettier --write"
  }
}

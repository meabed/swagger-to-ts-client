{
  "name": "swagger-to-typescript-client",
  "version": "2.1.6",
  "private": false,
  "description": "Generate TypeScript axios client from Swagger & OpenAPI specification",
  "keywords": [
    "Swagger",
    "OpenAPI",
    "OpenAPI 3",
    "OpenAPI-To-TypeScript",
    "Swagger-To-TypeScript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/meabed/swagger-to-typescript-client"
  },
  "license": "MIT",
  "author": "Mohamed Meabed <mo@meabed.com> (https://meabed.com)",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "gen-sdk": "./dist/index.js"
  },
  "files": [
    "dist/*",
    "package.json",
    "yarn.lock"
  ],
  "scripts": {
    "build": "rm -rf dist && tsc -p . --skipLibCheck && yarn copy-assets",
    "copy-assets": "cp -r template dist/template/",
    "lint": "node_modules/.bin/eslint \"src/**/*.ts\" \"./index.ts\" \"__tests__/**/*.ts\"",
    "prepublishOnly": "yarn build",
    "prettier": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\" \"__tests__/**/*.{ts,tsx,js,jsx}\" ",
    "start": "node_modules/.bin/ts-node --transpile-only index.ts",
    "test": "NODE_ENV=test jest --runInBand --forceExit"
  },
  "lint-staged": {
    "src/**/*.{ts,json}": "prettier --write",
    "__tests__/**/*.{ts}": "prettier --write",
    "./index.ts": "prettier --write"
  },
  "dependencies": {
    "camelcase": "7.0.0",
    "ncp": "^2.0.0",
    "openapi-client-axios-typegen": "5.3.1",
    "openapi-types": "^12.0.2",
    "prompts": "2.4.2",
    "rimraf": "3.0.2",
    "swagger-parser": "10.0.3",
    "ts-node": "10.9.1",
    "typescript": "4.8.4",
    "yargs": "17.6.0"
  },
  "devDependencies": {
    "@types/eslint": "8.4.8",
    "@types/eslint-plugin-prettier": "3.1.0",
    "@types/jest": "29.2.0",
    "@types/ncp": "2.0.5",
    "@types/node": "18.11.5",
    "@types/prettier": "2.7.1",
    "@types/prompts": "2.4.1",
    "@types/rimraf": "3.0.2",
    "@types/swagger-schema-official": "2.0.22",
    "@types/yargs": "17.0.13",
    "@typescript-eslint/eslint-plugin": "5.41.0",
    "@typescript-eslint/parser": "5.41.0",
    "eslint": "8.26.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.1",
    "jest": "29.2.2",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "swagger-schema-official": "2.0.0-bab6bed",
    "ts-jest": "next"
  }
}

{
  "name": "swagger-to-typescript-client",
  "version": "2.0.0",
  "license": "UNLICENSED",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "gen-sdk": "./dist/index.js"
  },
  "files": [
    "dist/*",
    "package.json",
    "yarn.lock"
  ],
  "scripts": {
    "build": "rm -rf dist && tsc -p . --skipLibCheck && yarn copy-assets",
    "copy-assets": "cp -r template dist/template/",
    "lint": "node_modules/.bin/eslint \"src/**/*.ts\" \"./index.ts\" \"__tests__/**/*.ts\"",
    "prepublishOnly": "yarn build",
    "prettier": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\" \"__tests__/**/*.{ts,tsx,js,jsx}\" ",
    "start": "node_modules/.bin/ts-node --transpile-only index.ts",
    "test": "NODE_ENV=test jest --runInBand --forceExit"
  },
  "lint-staged": {
    "src/**/*.{ts,json}": "prettier --write",
    "__tests__/**/*.{ts}": "prettier --write",
    "./index.ts": "prettier --write"
  },
  "dependencies": {
    "camelcase": "6.2.1",
    "ncp": "^2.0.0",
    "openapi-client-axios-typegen": "4.2.1",
    "openapi-types": "^9.3.1",
    "prompts": "2.4.2",
    "rimraf": "3.0.2",
    "swagger-parser": "10.0.3",
    "ts-node": "10.4.0",
    "typescript": "4.4.4",
    "yargs": "17.2.1"
  },
  "devDependencies": {
    "@types/eslint": "7.28.2",
    "@types/eslint-plugin-prettier": "3.1.0",
    "@types/jest": "27.0.2",
    "@types/ncp": "2.0.5",
    "@types/node": "16.11.7",
    "@types/prettier": "2.4.2",
    "@types/prompts": "2.0.14",
    "@types/rimraf": "3.0.2",
    "@types/swagger-schema-official": "2.0.22",
    "@types/yargs": "17.0.5",
    "@typescript-eslint/eslint-plugin": "5.3.1",
    "@typescript-eslint/parser": "5.3.1",
    "eslint": "8.2.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "7.0.4",
    "jest": "27.3.1",
    "lint-staged": "12.0.2",
    "prettier": "2.4.1",
    "swagger-schema-official": "2.0.0-bab6bed",
    "ts-jest": "next"
  }
}
